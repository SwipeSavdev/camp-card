{
  "expo": {
    "name": "campcard-mobile",
    "slug": "campcard-mobile",
    "version": "1.0.0",
    "description": "BSA Camp Card Mobile App - Digital discount subscription supporting Scout fundraising",
    "sdkVersion": "54.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true,
          "NSExceptionDomains": {
            "18.190.69.205": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            },
            "192.168.1.216": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            },
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            }
          }
        }
      }
    },
    "android": {
      "usesCleartextTraffic": true
    }
  }
}
